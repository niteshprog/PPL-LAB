/* using third variable*/
#include <stdio.h>
#include <stdlib.h>

int main()

{	
    int m, n; 
	printf("Enter the first no.");
	scanf("%d", &m);
	printf("Enter the second no.");
	scanf("%d", &n);
	int z= n;
	printf("%d, %d", z, m);
	
	
	return 0;
} 	
	
/* whithout third variable*/
#include <stdio.h>
#include <stdlib.h>

int main()
{
	int m,n;
	printf("Enter the first no.");
	scanf("%d", &m);
	printf("Enter the second no.");
	scanf("%d", &n);
	printf("%d, %d", n,m);
	
	return 0;
}
